    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        XYSeries oSeries = new XYSeries("Prueba");
        XYSeries oSeries2 = new XYSeries("Prueba");
        XYSeries oSeries3 = new XYSeries("Prueba");
        
        double resultado;
        
        double anterior=0.0;
        double actual=0.0;
        String dato="Inicia";
        
        String inflexion="";
        
        
        for (double i = -4.0; i <= 4.0; i+=0.1) {
            resultado=fx(i);
            
            oSeries.add(i,resultado);
        }
        System.out.println("f ser치 creciente  cuando f1(x) > 0, o cuando x>2");
        System.out.println("f ser치 una funci칩n decreciente cuando f1(x) < 0, o cuando x<2");
        System.out.println("f no ser치 creciente ni decreciente cuando f1(x) = 0, o cuando X=2");
        
        /*for (int i = 0; i < lista.size(); i++) {
            System.out.println("");
        }*/
        XYSeriesCollection oDataSet = new XYSeriesCollection();
        oDataSet.addSeries(oSeries);
        //oDataSet.addSeries(oSeries2);
        //oDataSet.addSeries(oSeries3);

        
        JFreeChart oChart = ChartFactory.createXYLineChart("Ejemplo grafica","X","Y", oDataSet,PlotOrientation.VERTICAL, true, false, false);
        
        ChartPanel oPanel = new ChartPanel(oChart);
        
        pnlMain.setLayout(new java.awt.BorderLayout());
        pnlMain.add(oPanel);
        pnlMain.validate();
        
        
    }                                        
    
    public static double fx(double x){
        return 5*x*x-20*x+3;
    }
    public static double f1x(double x){
        return 10*x-20;
    }
    public static double f2x(double x){
        return 10;
    }
